   "source": [
    "N = int(input())\n",
    "c = 0\n",
    "if N==0:\n",
    "    print(0)\n",
    "    \n",
    "\n",
    "    \n",
    "def dec_to_bin(N):\n",
    "\n",
    "    if (N>=1):\n",
    "        global c\n",
    "        dec_to_bin(N//2)\n",
    "        c = N%2\n",
    "        print(c,end='')\n",
    "\n",
    "            \n",
    "if __name__ == '__main__':\n",
    "    dec_to_bin(N)\n",
    "\n",
    "\n"
   ]
  }
